    filter {
        ip-filter "rs_protection_v4" {
            entry 10 {
                match {
                    src-ip {
                        address 80.81.192.0/21
                    }
                    dst-ip {
                        address 80.81.192.0/21
                    }
                    protocol icmp
                }
                action {
                    accept
                }
            }
            entry 30 {
                match {
                    src-ip {
                        address 80.81.192.0/21
                    }
                    dst-ip {
                        address 80.81.192.0/21
                    }
                    protocol tcp
                    src-port {
                        eq 179
                    }
                }
                action {
                    accept
                }
            }
            entry 31 {
                match {
                    src-ip {
                        address 80.81.192.0/21
                    }
                    dst-ip {
                        address 80.81.192.0/21
                    }
                    protocol tcp
                    dst-port {
                        eq 179
                    }
                }
                action {
                    accept
                }
            }
            entry 40 {
                match {
                    src-ip {
                        address 80.81.192.0/21
                    }
                    dst-ip {
                        address 80.81.192.0/21
                    }
                    protocol udp
                    src-port {
                        eq 3784
                    }
                }
                action {
                    accept
                }
            }
            entry 41 {
                match {
                    src-ip {
                        address 80.81.192.0/21
                    }
                    dst-ip {
                        address 80.81.192.0/21
                    }
                    protocol udp
                    dst-port {
                        eq 3784
                    }
                }
                action {
                    accept
                }
            }
            default-action drop
        }
        ipv6-filter "rs_protection_v6" {
            entry 10 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address 2001:7f8::/64
                    }
                    next-header ipv6-icmp
                }
                action {
                    accept
                }
            }
            entry 11 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address fe80::/10
                    }
                    next-header ipv6-icmp
                }
                action {
                    accept
                }
            }
            entry 12 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address ff00::/8
                    }
                    next-header ipv6-icmp
                }
                action {
                    accept
                }
            }
            entry 20 {
                match {
                    src-ip {
                        address fe80::/10
                    }
                    dst-ip {
                        address 2001:7f8::/64
                    }
                    next-header ipv6-icmp
                }
                action {
                    accept
                }
            }
            entry 21 {
                match {
                    src-ip {
                        address fe80::/10
                    }
                    dst-ip {
                        address fe80::/10
                    }
                    next-header ipv6-icmp
                }
                action {
                    accept
                }
            }
            entry 22 {
                match {
                    src-ip {
                        address fe80::/10
                    }
                    dst-ip {
                        address ff00::/8
                    }
                    next-header ipv6-icmp
                }
                action {
                    accept
                }
            }
            entry 30 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address 2001:7f8::/64
                    }
                    next-header tcp
                    src-port {
                        eq 179
                    }
                }
                action {
                    accept
                }
            }
            entry 31 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address 2001:7f8::/64
                    }
                    next-header tcp
                    dst-port {
                        eq 179
                    }
                }
                action {
                    accept
                }
            }
            entry 40 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address 2001:7f8::/64
                    }
                    next-header udp
                    src-port {
                        eq 3784
                    }
                }
                action {
                    accept
                }
            }
            entry 41 {
                match {
                    src-ip {
                        address 2001:7f8::/64
                    }
                    dst-ip {
                        address 2001:7f8::/64
                    }
                    next-header udp
                    dst-port {
                        eq 3784
                    }
                }
                action {
                    accept
                }
            }
            default-action drop
        }
    }
    router * {
        interface * {
            egress {
                filter {
                    ip "rs_protection_v4"
                    ipv6 "rs_protection_v6"
                }
            }
        }
    }