        }
        entry 40 {
            match {
                ipv6 {
                    source-ip {
                        prefix 2001:7f8::/64
                    }
                    destination-ip {
                        prefix 2001:7f8::/64
                    }
                    next-header udp
                }
                transport {
                    source-port {
                        value bfd
                    }
                }
            }
            action {
                accept {
