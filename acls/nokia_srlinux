        }
        entry 41 {
            match {
                ipv6 {
                    source-ip {
                        prefix 2001:7f8::/64
                    }
                    destination-ip {
                        prefix 2001:7f8::/64
                    }
                    protocol udp
                }
                transport {
                    destination-port {
                        value bfd
                    }
                }
            }
            action {
                accept {
                }
            }
        }
        entry 50 {
            action {
                drop {
                }
            }
        }
    }
    interface * {
        output {
            acl-filter rs_protection_v4 type ipv4 {
            }
            acl-filter rs_protection_v6 type ipv6 {
            }
        }
    }
}